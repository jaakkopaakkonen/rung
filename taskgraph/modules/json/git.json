[
  {
    "target": "branch",
    "executable": "git",
    "commandLineArguments": "branch --show-current"
  },
  {
    "target": "remote",
    "executable": "git",
    "commandLineArguments": "remote"
  },
  {
    "target": "addUpdatedToGit",
    "executable": "git",
    "commandLineArguments": "add --update"
  },
  {
    "target": "commit",
    "executable": "git",
    "commandLineArguments": [
      "commit",
      " --file={commitMessageFile}",
      " --message=\"{commitMessage}\""
    ],
    "optionalInputs" : ["commitMessageFile", "commitMessage"]
  },
  {
    "target": "pushToGitlab",
    "executable": "git",
    "commandLineArguments": [
      "push",
      " -o merge_request.create ",
      " -o merge_request.target={targetBranch}",
      " {remote}",
      " {branch}",
      ":{featureBranch}"
    ],
    "inputs": ["remote"],
    "optionalInputs": ["branch", "featureBranch", "targetBranch"]
  },
  {
    "target": "commitId",
    "executable": "git",
    "commandLineArguments": [
      "rev-parse HEAD"
    ]
  }
]

