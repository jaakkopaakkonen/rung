[
  {
    "target": "branch",
    "executable": "git",
    "command_line_arguments": "branch --show-current"
  },
  {
    "target": "remote",
    "executable": "git",
    "command_line_arguments": "remote"
  },
  {
    "target": "addUpdatedToGit",
    "executable": "git",
    "command_line_arguments": "add --update"
  },
  {
    "target": "commit",
    "executable": "git",
    "command_line_arguments": ["commit", "--file={commitMessageFile}", "--message=\"{commitMessage}\""],
    "optional_input_names" : ["commitMessageFile", "commitMessage"]
  },
  {
    "target": "pushToGitlab",
    "executable": "git",
    "command_line_arguments": "push -o merge_request.create -o merge_request.target={targetBranch} {remote} {branch}:{featureBranch}",
    "input_names": ["branch", "featureBranch", "targetBranch", "remote"]
  }
]

