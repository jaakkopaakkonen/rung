[
  {
    "target": "listNetworkConnections",
    "executable": "nmcli",
    "command_line_arguments": "-terse connection"
  },
  {
    "target": "networkConnection",
    "executable": "nmcli",
    "command_line_arguments": "connection up {connection}",
    "input_names": ["connection"]
  },
  {
    "target": "wifi",
    "executable": "nmcli",
    "command_line_arguments": "radio wifi {state}",
    "input_names": ["state"]
  },
  {
    "target": "connectWifi",
    "executable": "nmcli",
    "command_line_arguments": "device wifi connect {SSID} password \"{password}\"",
    "input_names": ["SSID", "password"]
  }
]
