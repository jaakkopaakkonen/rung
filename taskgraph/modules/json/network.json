[
  {
    "target": "list_connections",
    "executable": "nmcli",
    "command_line_arguments": "-terse connection"
  },
  {
    "target": "vpn",
    "executable": "nmcli",
    "command_line_arguments": "connection up {vpn_network}",
    "input_names": ["vpn_network"]
  },
  {
    "target": "wifi",
    "executable": "nmcli",
    "command_line_arguments": "radio wifi on"
  },
  {
    "target": "connect_wifi_network",
    "executable": "nmcli",
    "command_line_arguments": "device wifi connect {SSID} password \"{password}\"",
    "input_names": ["SSID", "password"]
  }
]
