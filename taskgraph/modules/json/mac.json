[
  {
    "target": "lockScreen",
    "executable": "pmset",
    "commandLineArguments": "displaysleepnow"
  },
  {
    "target": "sleep",
    "executable": "pmset",
    "commandLineArguments": "sleepnow"
  },
  {
    "target": "networkInterfaces",
    "executable": "networksetup",
    "commandLineArguments": "-listallhardwareports",
    "postprocess": {
      "name": "^Hardware Port: (.*)",
      "interface": "^Device: (.*)",
      "macAddress": "^Ethernet Address: (.*)"
    }
  }
]
