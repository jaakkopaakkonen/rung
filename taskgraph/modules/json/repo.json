[
  {
    "target": "init",
    "executable": "repo",
    "commandLineArguments": [
      "init ",
      "--manifest-url={url} ",
      "--manifest-branch={branch} ",
      "--manifest-name={manifestName} ",
      "--platform=auto --current-branch"
    ],
    "inputs": ["url", "branch", "manifestName"]
  },
  {
    "target": "sync",
    "executable": "repo",
    "commandLineArguments": "sync"
  },
  {
    "target": "effectiveManifest",
    "executable": "repo",
    "commandLineArguments": "manifest"
  }
]

