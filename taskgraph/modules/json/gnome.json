[
  {
    "target": "lockScreen",
    "input_names": ["sendDbusMessage"],
    "values": {
      "type":  "method_call",
      "destination": "org.gnome.ScreenSaver",
      "contents": "/org/gnome/ScreenSaver org.gnome.ScreenSaver.Lock"
    }
  },
  {
    "target": "terminalWindow",
    "executable": "gnome-terminal",
    "command_line_arguments": "--window"
  }
]
